    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@cadl-lang/compiler": "^0.28.0",
    "tslib": "^2.3.1"
  }
